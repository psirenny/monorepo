  "scripts": {
    "audit": "yarn audit",
    "build": "rollup -c",
    "check": "run-s audit lint typecheck",
    "clean": "trash **.DS_Store *.log dist node_modules",
    "flow:install": "flow-typed update",
    "lint": "run-s lint:js lint:md lint:pkg",
    "lint:js": "eslint . --ext .js,.md",
    "lint:md": "remark --frail .",
    "lint:pkg": "npmPkgJsonLint ./package.json",
    "outdated": "yarn outdated",
    "postupdate": "run-s flow:install",
    "typecheck": "flow check .",
    "update": "yarn upgrade-interactive --latest",
    "watch": "rollup -c -w"
