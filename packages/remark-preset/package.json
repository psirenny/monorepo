  "scripts": {
    "audit": "nsp check",
    "build": "rollup -c ../rollup-config_node_tool/src/index.js src/index.js",
    "check": "run-s audit lint typecheck typecheck:cov",
    "clean": "trash **.DS_Store *.log dist dist node_modules",
    "lint": "run-s lint:js lint:md lint:pkg",
    "lint:js": "eslint . --ext .js,.md",
    "lint:md": "remark --frail .",
    "lint:pkg": "npmPkgJsonLint ./package.json",
    "outdated": "ncu -t",
    "typecheck": "flow .",
    "typecheck:cov": "flow-coverage-report --config .flowcoveragereportrc",
    "update": "ncu -tu"
