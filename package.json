{
  "name": "@psirenny/monorepo",
  "description": "@psirenny's monorepo",
  "version": "1.1.0",
  "author": "Dennis Torres <djtorres0@gmail.com>",
  "bugs": {
    "url": "https://github.com/psirenny/psirenny/issues"
  },
  "devDependencies": {
    "@commitlint/cli": "7.1.2",
    "@commitlint/config-conventional": "7.1.2",
    "@commitlint/config-lerna-scopes": "7.1.2",
    "circleci-bin": "1.0.0",
    "fixpack": "2.3.1",
    "flow-mono-cli": "1.4.0",
    "greenkeeper-lockfile": "2.5.0",
    "husky": "1.0.1",
    "lerna": "3.3.1",
    "npm-check-updates": "3.0.0-alpha.3",
    "npm-run-all": "4.1.3",
    "nsp": "3.2.1",
    "remark-cli": "5.0.0",
    "remark-lint-blockquote-indentation": "1.0.2",
    "remark-lint-checkbox-character-style": "1.0.2",
    "remark-lint-code-block-style": "1.0.2",
    "remark-lint-emphasis-marker": "1.0.2",
    "remark-lint-fenced-code-marker": "1.0.2",
    "remark-lint-heading-style": "1.0.2",
    "remark-lint-link-title-style": "1.0.2",
    "remark-lint-rule-style": "1.0.2",
    "remark-lint-strong-marker": "1.0.2",
    "remark-lint-table-cell-padding": "1.0.2",
    "remark-preset-lint-consistent": "2.0.2",
    "remark-preset-lint-markdown-style-guide": "2.1.2",
    "remark-preset-lint-recommended": "3.0.2",
    "remark-retext": "3.1.1",
    "retext-assuming": "1.0.0",
    "retext-contractions": "2.1.2",
    "retext-diacritics": "1.2.1",
    "retext-english": "3.0.0",
    "retext-equality": "3.4.1",
    "retext-indefinite-article": "1.1.4",
    "retext-intensify": "3.0.1",
    "retext-passive": "1.3.0",
    "retext-profanities": "4.4.0",
    "retext-quotes": "2.0.1",
    "retext-readability": "4.2.0",
    "retext-redundant-acronyms": "1.2.1",
    "retext-repeated-words": "1.2.1",
    "retext-sentence-spacing": "2.0.1",
    "retext-simplify": "4.1.1",
    "retext-spell": "2.3.1",
    "retext-syntax-mentions": "1.1.4",
    "retext-syntax-urls": "1.0.0",
    "trash-cli": "1.4.0",
    "unified": "7.0.0"
  },
  "homepage": "https://github.com/psirenny/monorepo",
  "keywords": [
    "mono",
    "psirenny",
    "repo"
  ],
  "license": "Unlicense",
  "main": "packages",
  "private": true,
  "repository": "https://github.com/psirenny/monorepo",
  "scripts": {
    "audit:self": "nsp check",
    "build": "npm-run-all build:pkgs",
    "build:pkgs": "lerna run build",
    "check": "npm-run-all check:self check:pkgs",
    "check:pkgs": "lerna run check",
    "check:self": "npm-run-all audit:self ci:check fixpack:self",
    "ci:check": "circleci config validate",
    "ci:postcheck": "greenkeeper-lockfile-upload",
    "ci:precheck": "greenkeeper-lockfile-update",
    "clean": "npm-run-all clean:pkgs clean:self",
    "clean:pkgs": "lerna run clean",
    "clean:self": "trash **.DS_Store *.log node_modules",
    "fixpack:self": "fixpack",
    "flow-fix": "flow-mono create-symlinks ./build/.flowconfig",
    "install:pkgs": "npm-run-all lerna:bootstrap flow-fix",
    "lerna:bootstrap": "lerna bootstrap",
    "lint:self": "remark --frail .",
    "outdated": "npm-run-all outdated:self outdated:pkgs",
    "outdated:pkgs": "lerna run outdated",
    "outdated:self": "ncu",
    "publish": "lerna publish",
    "release": "lerna version",
    "update": "npm-run-all update:self update:pkgs",
    "update:pkgs": "lerna run update",
    "update:self": "ncu -u"
  },
  "workspaces": [
    "packages/*"
  ]
}
