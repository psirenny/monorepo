{
  "name": "@psirenny/monorepo",
  "version": "1.0.0",
  "description": "@psirenny's monorepo",
  "author": "Dennis Torres <djtorres0@gmail.com>",
  "bugs": "https://github.com/psirenny/monorepo/issues",
  "devDependencies": {
    "@commitlint/cli": "8.0.0",
    "@commitlint/config-conventional": "8.0.0",
    "@commitlint/config-lerna-scopes": "8.0.0",
    "circleci-bin": "1.1.8",
    "flow-mono-cli": "1.5.0",
    "greenkeeper-lockfile": "2.8.0",
    "husky": "2.3.0",
    "lerna": "3.14.1",
    "npm-package-json-lint": "3.6.1",
    "npm-run-all": "4.1.5",
    "remark-cli": "6.0.1",
    "remark-lint-blockquote-indentation": "1.0.2",
    "remark-lint-checkbox-character-style": "1.0.2",
    "remark-lint-code-block-style": "1.0.2",
    "remark-lint-emphasis-marker": "1.0.2",
    "remark-lint-fenced-code-marker": "1.0.2",
    "remark-lint-heading-style": "1.0.2",
    "remark-lint-link-title-style": "1.0.3",
    "remark-lint-rule-style": "1.0.2",
    "remark-lint-strong-marker": "1.0.2",
    "remark-lint-table-cell-padding": "1.0.3",
    "remark-preset-lint-consistent": "2.0.2",
    "remark-preset-lint-markdown-style-guide": "2.1.2",
    "remark-preset-lint-recommended": "3.0.2",
    "remark-retext": "3.1.2",
    "retext-assuming": "1.0.0",
    "retext-contractions": "2.1.3",
    "retext-diacritics": "1.2.2",
    "retext-english": "3.0.2",
    "retext-equality": "3.10.0",
    "retext-indefinite-article": "1.1.6",
    "retext-intensify": "4.0.0",
    "retext-passive": "1.3.2",
    "retext-profanities": "4.6.0",
    "retext-quotes": "2.0.3",
    "retext-readability": "4.2.1",
    "retext-redundant-acronyms": "1.2.2",
    "retext-repeated-words": "1.2.2",
    "retext-sentence-spacing": "2.1.0",
    "retext-simplify": "4.1.2",
    "retext-spell": "2.4.0",
    "retext-syntax-mentions": "1.1.5",
    "retext-syntax-urls": "1.0.1",
    "trash-cli": "3.0.0",
    "unified": "7.1.0"
  },
  "homepage": "https://github.com/psirenny/monorepo",
  "keywords": [
    "monorepo",
    "psirenny"
  ],
  "license": "Unlicense",
  "private": true,
  "repository": "https://github.com/psirenny/monorepo",
  "scripts": {
    "audit": "yarn audit",
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build",
    "check": "run-s check:root check:pkgs",
    "check:pkgs": "lerna run check",
    "check:root": "run-s audit deps:check lint",
    "ci:check": "circleci local execute --job build_and_check",
    "ci:postcheck": "greenkeeper-lockfile-upload",
    "ci:precheck": "greenkeeper-lockfile-update",
    "clean": "run-s clean:pkgs clean:root",
    "clean:pkgs": "lerna run clean",
    "clean:root": "trash **.DS_Store *.log node_modules",
    "deps:check": "run-s deps:check:verify deps:check:integrity",
    "deps:check:integrity": "yarn check --integrity",
    "deps:check:verify": "yarn check --verify-tree",
    "flow:fix": "flow-mono create-symlinks ./build/.flowconfig",
    "flow:install": "lerna run flow:install",
    "lint": "run-s lint:md lint:pkg",
    "lint:md": "remark --frail .",
    "lint:pkg": "npmPkgJsonLint ./package.json",
    "outdated": "yarn outdated",
    "postbootstrap": "flow-mono create-symlinks ./build/.flowconfig",
    "postinstall": "run-s bootstrap",
    "postupdate": "lerna run postupdate",
    "prepublish": "run-s build",
    "publish": "lerna publish",
    "update": "run-s update:js update:yarn",
    "update:js": "yarn upgrade-interactive --latest",
    "update:yarn": "yarn policies set-version"
  },
  "workspaces": [
    "packages/*"
  ]
}
